<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="helloSpring" class="com.example.demo.HelloSpring">
        <property name="userName" value="张三"/>
    </bean>

    <bean id="user1" class="com.example.demo.User1">
        <constructor-arg type="int" value="001"/>
        <constructor-arg type="java.lang.String" value="abc"/>
        <constructor-arg type="java.lang.String" value="90329032"/>
 </bean>



</beans>

